<html>
<head>
    <title>Internet Archive</title>
</head>

<body TOPMARGIN="0" LEFTMARGIN="0" RIGHTMARGIN="0" BOTTOMMARGIN="0" link="#999999" bgcolor="#f5f5f7" vlink="#999999">
    <font face="Arial">
        <table BGCOLOR="#222222" BORDER=0 CELLSPACING=0 CELLPADDING=6 width="100%" height="24">
            <%- include('partials/header') %>
        </table>

        <table cellspacing="5" cellpadding="3" width="100%">
            <tr>
                <td width="50" height="50" align="center" valign="middle"><img src="/services/img/<%= identifier %>?w=50&h=50"></td>
                <td valign="middle">
                    <font size=6><b><%= title %></b></font>
                    <% if (creator) { %>
                    <br>by <a href="/search?query=<%= encodeURIComponent(`creator:"${creator}"`) %>"><%= creator %></a>
                    <% } %>
                </td>
            </tr>
        </table>

        <table BORDER="0" CELLSPACING=0 CELLPADDING=5 width="100%" bgcolor="#FFFFFF">
            <% if (locals.results) { %>
            <tr>
                <td colspan="2">
                    <p><%= numFound %> items</p>
                </td>
            </tr>
            <% results.forEach(result => { %>
            <tr>
                <td width=24>
                    <img src="/<%- result.mediatype %>.gif">
                </td>
                <td valign="middle">
                    <a href="/details/<%= result.identifier %>">
                        <%= result.title %>
                    </a>
                </td>
            </tr> <% }) %> </td>
            </tr>
        </table>
        <% if (locals.page) { %>
        <center>
            <% if (page> 1) { %> <a href="?page=<%= (page-1) %>">Prev page</a> | <% } %> <a href="?page=<%= (page+1) %>">Next page</a>
        </center>
        <% } %>
        <% } %>
        <br>
        <%- include('partials/footer') %>
    </font>
</body>
</html>