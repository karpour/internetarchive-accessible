<html>
<head>
    <title>Internet Archive</title>
</head>

<body TOPMARGIN="0" LEFTMARGIN="0" RIGHTMARGIN="0" BOTTOMMARGIN="0" link="#999999" bgcolor="#f5f5f7" vlink="#999999">
    <font face="Arial">
        <table BGCOLOR="#222222" BORDER=0 CELLSPACING=0 CELLPADDING=6 width="100%" height="24">
            <%- include('partials/header') %>
            <tr BGCOLOR="#000000">
                <td align="center" colspan="2">
                    <img src="/services/img/<%= identifier %>" alt="Item image" />
                </td>
            </tr>
        </table>
        <table BORDER="0" CELLSPACING=10 CELLPADDING=5 width="100%">
            <tr>
                <td>

                    <h2>Files for <a href="/details/<%= identifier %>"><%= identifier %></a></h2>
                    <tt>
                        Last Modified&nbsp;&nbsp;&nbsp;Size&nbsp;&nbsp;&nbsp;Name<br>
                        ------------------------------<br>
                        <% files.forEach(f => { %>
                        <%= f.date %>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%- f.size.padEnd(7," ").replaceAll(" ","&nbsp;") %><a href="https://archive.org/download/<%= identifier %>/<%= f.name %>"><br><%= f.name %></a><br>
                        <% }) %>
                    </tt>

                </td>
            </tr>
        </table>
        <center>
            <%- include('partials/footer') %>
        </center>
    </font>
</body>
</html>